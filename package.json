{
  "name": "profile",
  "private": true,
  "scripts": {
    "boot": "lerna bootstrap",
    "build": "lerna run build",
    "dev": "lerna run --scope '@profile/{server,editor}' --parallel dev",
    "build:editor": "lerna run build:editor --scope @profile/editor",
    "build:preview": "lerna run build:preview --scope @profile/editor",
    "server:build": "lerna run build --scope @profile/server",
    "clean": "lerna clean"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "8.0.3",
    "lerna": "5.1.8",
    "lint-staged": "13.1.2",
    "prettier": "^3.2.5",
    "ts-node": "10.0.0",
    "typescript": "4.4.3"
  }
}
